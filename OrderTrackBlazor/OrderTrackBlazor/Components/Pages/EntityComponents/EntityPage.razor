@page "/entity/{EntityName}"
@rendermode InteractiveServer
@using Microsoft.AspNetCore.Authorization
@attribute [Authorize]
@inherits EntityPageComponent
<h3>Entity @EntityName</h3>


@if (Shops == null)
{
  <div>null</div>
}
else
{
  <div>
    <Table TItem="OrderTrackShop" Items="@Shops">
      <TableColumns>
        <TableColumn @bind-Field="@context.ShopType" Width="180" />
        <TableColumn @bind-Field="@context.PostCode" />
        <TableColumn @bind-Field="@context.Note" />
      </TableColumns>
    </Table>
  </div>
}